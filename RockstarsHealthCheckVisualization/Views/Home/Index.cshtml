@model RockstarsHealthCheckVisualization.Models.Date
@{
    ViewData["Title"] = "Home Page";
}
@using (Html.BeginForm("Checkpoint", "Home", FormMethod.Post))
{
    <input type="submit" value="Submit" />

}
<div class="text-center">
    <h1 class="display-4">Welcome to visualization</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    @if (Model != null)
    {
        <p>Current login: @Model.checkpoint.ToString("dd-MMM-yyyy hh:mm tt")</p>
        <p>Latest login: @Model.latestDateTime </p>
    }
</div>
